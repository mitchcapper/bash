PACKAGE = @PACKAGE_NAME@
VERSION = @PACKAGE_VERSION@

PACKAGE_BUGREPORT = @PACKAGE_BUGREPORT@
PACKAGE_NAME = @PACKAGE_NAME@
PACKAGE_STRING = @PACKAGE_STRING@
PACKAGE_VERSION = @PACKAGE_VERSION@

srcdir = @srcdir@
VPATH = @srcdir@
topdir = @top_srcdir@
BUILD_DIR = @BUILD_DIR@
OBJEXT = @OBJEXT@

LIBBUILD = ${BUILD_DIR}/gnu ${BUILD_DIR}/lib

BASHINCDIR = ${topdir}/include

INTL_LIBSRC = ${topdir}/lib/intl
INTL_BUILDDIR = ${LIBBUILD}/intl
INTL_INC = @INTL_INC@
LIBINTL_H = @LIBINTL_H@

datarootdir = @datarootdir@

INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_DATA = @INSTALL_DATA@

CC = @CC@
RANLIB = @RANLIB@
AR = @AR@
ARFLAGS = @ARFLAGS@
RM = rm -f
CP = cp
MV = mv

SHELL = @MAKE_SHELL@

CFLAGS = @CFLAGS@ -DHAVE_CONFIG_H -DHAVE_UNISTD_H -DHAVE_PSELECT -DHAVE_SYS_RANDOM_H -DHAVE_GETENTROPY
LOCAL_CFLAGS = @LOCAL_CFLAGS@ ${DEBUG}
STYLE_CFLAGS = @STYLE_CFLAGS@
CPPFLAGS = @CPPFLAGS@
LDFLAGS = @LDFLAGS@ @LOCAL_LDFLAGS@

PROFILE_FLAGS = @PROFILE_FLAGS@

DEFS = @DEFS@
LOCAL_DEFS = @LOCAL_DEFS@

INCLUDES = -I. -I../.. -I$(topdir) -I$(topdir)/gnu -I$(topdir)/lib -I$(BASHINCDIR) -I$(srcdir) $(INTL_INC)

CCFLAGS = ${ADDON_CFLAGS} ${PROFILE_FLAGS} ${INCLUDES} $(DEFS) $(LOCAL_DEFS) \
	  $(LOCAL_CFLAGS) $(STYLE_CFLAGS) $(CFLAGS) $(CPPFLAGS) 

GCC_LINT_FLAGS = -Wall -Wshadow -Wpointer-arith -Wcast-qual \
		 -Wcast-align -Wstrict-prototypes -Wconversion \
		 -Wmissing-prototypes -Wtraditional -Wredundant-decls -pedantic

%.obj: %.c
	${RM} $@
	$(CC) $(CCFLAGS) -c $<

# The name of the library target.
LIBRARY_NAME = libsh.a

# The C code source files for this library.
#strcasecmp.c strerror.c strtod.c strtol.c wcswidth.c mbschr.c mktime.c  gettimeofday.c  strftime.c  fpurge.c  inet_aton.c   strcasestr.c snprintf.c mbscasecmp.c  strdup.c   memset.c strstr.c
CSOURCES = clktck.c clock.c getcwd.c getenv.c oslib.c setlinebuf.c timers.c \
	   strtoul.c  itos.c timeval.c \
	   vprint.c rename.c zread.c zwrite.c shtty.c \
	   netconn.c netopen.c strpbrk.c  makepath.c \
	   pathcanon.c pathphys.c tmpfile.c stringlist.c stringvec.c spell.c \
	   shquote.c strtrans.c mailstat.c \
	   fmtulong.c fmtullong.c fmtumax.c shmatch.c strnlen.c \
	   strtoll.c strtoull.c strtoimax.c strtoumax.c\
	    zcatfd.c zmapfd.c winsize.c eaccess.c \
	   wcsdup.c zgetline.c mbscmp.c uconvert.c ufuncs.c \
	   casemod.c dprintf.c input_avail.c fnxform.c \
	   strchrnul.c unicode.c  wcsnwidth.c shmbchar.c \
	   strvis.c utf8.c random.c

# The header files for this library.
HSOURCES = 

# The object files contained in $(LIBRARY_NAME)
#LIBOBJS = @LIBOBJS@
#  mbscmp.$(OBJEXT) getenv.$(OBJEXT) strnlen.$(OBJEXT)  gettimeofday.$(OBJEXT)   snprintf.$(OBJEXT) fpurge.$(OBJEXT)
OBJECTS = clktck.$(OBJEXT) clock.$(OBJEXT) timeval.$(OBJEXT) oslib.$(OBJEXT) tmpfile.$(OBJEXT) setlinebuf.$(OBJEXT) timers.$(OBJEXT)  \
	  itos.$(OBJEXT) zread.$(OBJEXT) zwrite.$(OBJEXT) shtty.$(OBJEXT) shmatch.$(OBJEXT) eaccess.$(OBJEXT) \
	  netconn.$(OBJEXT) netopen.$(OBJEXT) makepath.$(OBJEXT) pathcanon.$(OBJEXT) \
	  pathphys.$(OBJEXT)  stringlist.$(OBJEXT) stringvec.$(OBJEXT) spell.$(OBJEXT) shquote.$(OBJEXT) \
	  strtrans.$(OBJEXT) mailstat.$(OBJEXT) fmtulong.$(OBJEXT) \
	  fmtullong.$(OBJEXT) fmtumax.$(OBJEXT) zcatfd.$(OBJEXT) zmapfd.$(OBJEXT) winsize.$(OBJEXT) wcsdup.$(OBJEXT) \
	  zgetline.$(OBJEXT) uconvert.$(OBJEXT) ufuncs.$(OBJEXT) casemod.$(OBJEXT) \
	  input_avail.$(OBJEXT) mbscasecmp.$(OBJEXT) fnxform.$(OBJEXT) unicode.$(OBJEXT) shmbchar.$(OBJEXT) strvis.$(OBJEXT) \
	  utf8.$(OBJEXT) random.$(OBJEXT)  wcsnwidth.$(OBJEXT) ${LIBOBJS}

SUPPORT = Makefile

all: $(LIBRARY_NAME)

$(LIBRARY_NAME): $(OBJECTS)
	$(RM) $@
	$(AR) $(ARFLAGS) $@ $(OBJECTS)
	-test -n "$(RANLIB)" && $(RANLIB) $@

force:

# The rule for 'includes' is written funny so that the if statement
# always returns TRUE unless there really was an error installing the
# include files.
install:

clean:
	$(RM) $(OBJECTS) $(LIBRARY_NAME)

realclean distclean maintainer-clean: clean
	$(RM) Makefile

mostlyclean: clean

